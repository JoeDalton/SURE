#------------------
# DiffZ Flame Parameters
#------------------
BLOCK Parameters
  # Initial Solution
  Initial_Solution_File    = InitFlame.h5
  Initial_Solution_Format  = HDF

  # Output Solution(s)
  Freq_Iteration_Display   = 10
  Output_Level             = 1
  Output_Solution_Niter    = 100
  Output_Solution_Format   = HDF
  Output_Solution_Name     = Sols/Solution

  # Flame parameters
  Strain_Rate                     = 100.0
  Composition_Block_Z0            = Oxidizer 
  Composition_Block_Z1            = Fuel 
  UnityLewisNumbers               = YES #NO
  ActivateGrad_W                  = NO  #YES
  ActivateDiffusionCorrection     = NO  #YES
  ActivateFullDifferentialEffects = NO  #YES

  # Advanced parameters
  ODESolver_Parameters            = ODE_Parameters
  Grid_Adaptation_Parameters      = GRID_Parameters
END BLOCK

#------------------
# GRID Parameters
#------------------
BLOCK GRID_Parameters
  Adaptation = NO
END BLOCK

