#------------------
# Composition Z0 
#------------------
BLOCK Oxidizer 
  Mechanism        = SampledMech.h5
  CompoMode        = Classic 
  Temperature      = 1030
  Pressure         = 101325
  H2O	             = 0.0989
  N2               = 0.7534
  O2	             = 0.1474
  SpeciesFractions = MOLE
  Renormalize      = YES
END BLOCK

#------------------
# Composition Z1 
#------------------
BLOCK Fuel 
  Mechanism        = SampledMech.h5
  CompoMode        = Classic 
  Temperature      = 305
  Pressure         = 101325
  H2               = 0.2537
  N2	             = 0.7427
  SpeciesFractions = MOLE
  Renormalize      = YES
END BLOCK
